{
  "title": "web-dev-tools",
  "author": "wang yue",
  "language" : "zh-hans",
  "description" : "web前端开发常用的工具函数和小功能",
  "plugins": [
    "splitter",
    "-lunr",
    "-search",
    "search-plus",
    "tbfed-pagefooter",
    "expandable-chapters-small",
    "anchors",
    "anchor-navigation-ex",
    "edit-link",
    "copy-code-button",
    "-fontsettings"
  ],
  "pluginsConfig": {
    "tbfed-pagefooter": {
      "copyright": "&copy 920317438@qq.com 2018",
      "modify_label": "修订时间：",
      "modify_format": "2018-02-06 19:00:00"
    },
    "anchor-navigation-ex": {
      "isShowTocTitleIcon": true,
      "tocLevel1Icon": "fa fa-hand-o-right",
      "tocLevel2Icon": "fa fa-hand-o-right",
      "tocLevel3Icon": "fa fa-hand-o-right"
    },
    "edit-link": {
      "base": "https://github.com/musez/web-dev-tools/edit/dev",
      "label": "编辑此页"
    },
    "fontsettings": {
      "theme": "white",
      "family": "sans",
      "size": 2
    }
  }
}
